<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>Fable.ReactNative.DeviceInfo</PackageId>
    <Version>1.0.1</Version>
    <PackageTags>Fable;react-native;react-native-device-info;Fsharp;FSharp Fable ReactNative;</PackageTags>
    <Description>Fable bindings for react-native-device-info</Description>
    <PackageProjectUrl>https://github.com/martinmoec/Fable.ReactNative.DeviceInfo</PackageProjectUrl>
    <RepositoryUrl>https://github.com/martinmoec/Fable.ReactNative.DeviceInfo</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Fable.ReactNative.DeviceInfo.fs" />
  </ItemGroup>
  <PropertyGroup>
    <NpmDependencies>
      <NpmPackage Name="react-native-device-info" Version="&gt;= 5.6.0" />
    </NpmDependencies>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Fable.Core" Version="3.1.5" />
    <PackageReference Include="Fable.Promise" Version="2.0" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="*.fsproj; *.fs" PackagePath="fable\" />
  </ItemGroup>
</Project>